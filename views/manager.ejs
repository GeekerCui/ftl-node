<!DOCTYPE HTML>
<html>
<head>
	<meta charset="utf-8"/>
	<link rel="shortcut icon" href="<%=cdnBaseUrl%>/favicon.ico"/>
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
	<meta name="viewport" content="width=device-width, target-densitydpi=high-dpi, initial-scale=1.0, maximum-scale=1.0, user-scalable=no,minimal-ui" />
	<meta name="apple-mobile-web-app-capable" content="yes" />
	<meta name="apple-mobile-web-app-status-bar-style" content="black" />
	<meta content="telephone=no" name="format-detection" />
	<title>后台管理</title>
	<link rel="stylesheet" type="text/css" href="<%=cdnBaseUrl%>/css/base.css">
	<link rel="stylesheet" type="text/css" href="<%=cdnBaseUrl%>/lib/bootstrap/css/bootstrap.css">
	<link rel="stylesheet" type="text/css" href="<%=cdnBaseUrl%>/css/manager.css">
</head>
<body>
<nav class="navbar navbar-default" role="navigation">
	<div class="navbar-header">
		<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#nav">
			<span class="icon-bar"></span>
			<span class="icon-bar"></span>
			<span class="icon-bar"></span>
		</button>
		<a class="navbar-brand" href="<%=baseUrl%>/sys/manager.html">前端模拟环境</a>
	</div>
	<div class="collapse navbar-collapse" id="nav">
		<ul class="nav navbar-nav">
			<li class="active"><a href="<%=baseUrl%>/sys/manager.html">服务器配置</a></li>
		</ul>
	</div>
</nav>
<section class="panel panel-default">
	<div class="panel-heading">
			<div class="btn-group">
			<button class="btn btn-default new-group">新建分组</button>
			<button class="btn btn-default new-branch">新建分支</button>
			<button class="btn btn-default disabled-all">禁用全部</button>
			</div>
	</div>
	<div class="panel-body">
		<div class="container-fluid">
			<div class="row-fluid nav-row clearfix" onSelectStart="return false">
				<div class="col-sm-2 col-lg-1 silde-col">
					<nav class="sidebar hidden-print hidden-xs"></nav>
				</div>
				<div class="col-sm-10 col-lg-11 content-col"></div>
			</div>
			<div class="row-fluid">
				<div class="col-sm-12">
					<button type="button" class="btn btn-default pull-right save">应用</button>
				</div>
			</div>
		</div>
	</div>
</section>


<!-- 新建分组 -->
<div class="modal fade" id="create-group" role="dialog" tabindex="-1" >
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
				<h4 class="modal-title">新建分组</h4>
			</div>
		<div class="modal-body">
			<div class="form-horizontal clearfix">
				<div class="form-group">
					<label class="col-md-2 control-label">分组名称</label>
					<div class="col-md-10">
						<input type="text" class="form-control group-name" placeholder="请输入分组名称">
					</div>
				</div>
			</div>
		</div>
		<div class="modal-footer">
			<button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
			<button type="button" class="btn btn-primary confirm-create-group">确定</button>
		</div>
		</div>
	</div>
</div>

<!-- 新建分支 -->
<div class="modal fade" id="create-branch" role="dialog" tabindex="-1">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
				<h4 class="modal-title" >新建分支</h4>
			</div>
		<div class="modal-body">
			<div class="form-horizontal clearfix">
				<div class="form-group">
					<label class="col-md-2 control-label">分组名称</label>
					<div class="col-md-10">
						<div class="btn-group group-name-wrap">
							<input type="text" class="form-control dropdown-toggle group-name" placeholder="请输入分支名称" data-toggle="dropdown">
							<ul class="dropdown-menu group-name-menu" role="menu"></ul>
						</div>
					</div>
				</div>
				<div class="form-group">
					<label class="col-md-2 control-label">分支名称</label>
					<div class="col-md-10">
						<input type="text" class="form-control branch-name" placeholder="请输入分支名称">
					</div>
				</div>
			</div>
		</div>
		<div class="modal-footer">
			<button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
			<button type="button" class="btn btn-primary confirm-create-branch">确定</button>
		</div>
		</div>
	</div>
</div>
<script type="text/javascript">
	window.cdnBaseUrl = '<%=cdnBaseUrl%>';
	window.baseUrl = '<%=baseUrl%>';
</script>
<script type="text/javascript" src="<%=cdnBaseUrl%>/lib/jquery.js"></script>
<script type="text/javascript" src="<%=cdnBaseUrl%>/lib/bootstrap/js/bootstrap.js"></script>
<script type="text/javascript" src="<%=cdnBaseUrl%>/lib/Sortable.js"></script>
<script type="text/javascript" src="<%=cdnBaseUrl%>/lib/tpl.js"></script>
<script type="text/javascript" src="<%=cdnBaseUrl%>/js/manager.js"></script>
</body>
</html>